@model IEnumerable<AppMvc.Models.Cart>

@{
    ViewData["Title"] = "Index";
}

<h1>Carrinho</h1>


<table class="table">
    <thead>
        <tr>
            <th></th>
            <th>
                @Html.DisplayNameFor(model => model.TotalValue)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TotalValue)
            </td>
            <td>
            </td>
        </tr>
}
    </tbody>
</table>
